
<div class="container-fluid navbar">
  <ul class="nav-ul">
    <li class="nav-li">{{#link-to 'contact'}}Become a Host{{/link-to}}</li>
    <li class="nav-li">{{#link-to 'about'}}About{{/link-to}}</li>
    <li class="nav-li">{{#link-to 'contact'}}Contact{{/link-to}}</li>
  </ul>
</div>
<div class="container-fluid content">
  <div class="jumbotron">
    <h1><span id=liveThere>HomeStay.</span> Book unique homes and experience a city like a local.</h1>

    <input type="text" name="search" placeholder="Where to?">
    <input type="text" name="search" placeholder="Check In">
    <input type="text" name="search" placeholder="Check Out">

    <select>
      <option value="1 guest">1 guest</option>
      <option value="2 guests">2 guests</option>
      <option value="3 guests">3 guests</option>
      <option value="4 guests">4 guests</option>
      <option value="5+ guests">5+ guests</option>
    </select>
    <button class="btn btn-danger">Search</button>
  </div>
  <div class="faces row">
    <div class="col-md-7">
      <img class="image-banner" src="https://media.licdn.com/mpr/mpr/shrinknp_800_800/AAEAAQAAAAAAAAQiAAAAJDBiNzZmM2Y5LWM0ZTYtNDYyOC05YjNhLTY5NjMyY2M1NTg1OQ.jpg">
    </div>

    <div class="col-md-5">
      <div id="community">
        <p>The most important button on our platform. Learn more about the Airbnb Community Commitment.</p>
        <button type="button" class="community-button btn btn-danger">Accept</button>
      </div>
    </div>
  </div>

  <ul>
    {{#each model as |rental|}}
       {{rental-tile rental=rental}}
    {{/each}}
  </ul>

  {{new-rental saveRental2="saveRental3"}}

  <div class="Explore">Explore the world <br>
    <div class="large row">
      <div class="col-md-6">
        <div id="Paris">Paris</div>
        <div id="Rome">Rome<img src=""></div>
        <div id="LosAngeles"><img src="">Los Angeles</div>
      </div>
      <div class="col-md-6">
        <div id="Lisbon"><img src="">Lisbon</div>
        <div id="Tokyo"><img src="">Tokyo</div>
        <div id="London">London<img src=""></div>
      </div>
    </div>
  </div>
</div>
